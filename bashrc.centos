mgtags()
{
  git tag | grep TiMOS-MG |  sed 's/-\([[:digit:]]\+\)/ - \1/' | \
    sed -n 's/\(TiMOS-MG_[[:digit:]]\+_0_[[:alpha:]]\)\([[:digit:]]\+\)/\1 \2/p' | \
    sort -n -k 2 -k 4 -r | tr -d ' '
}


